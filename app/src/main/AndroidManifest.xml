<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.parkingappse">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".VerificationActivity"></activity>
        <activity android:name=".ParkingSlotActivity" />
        <activity android:name=".RegisterActivity" />
        <activity android:name=".ParkingReportDetailActivity" />
        <activity android:name=".ParkingReportActivity" />
        <activity android:name=".ParkingReceiptActivity" />
        <activity android:name=".ParkingActivity" />
        <activity android:name=".HomeActivity" />
        <activity android:name=".EditProfileActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>